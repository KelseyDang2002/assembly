    33                                  global fill_random_array
    34                                  extern rdrand
    35                                  
    36                                  segment .data
    37 00000000 256C6600                floatform db "%lf", 0
    38 00000004 257300                  stringform db "%s", 0
    39                                  
    40                                  segment .bss
    41                                  align 64
    42 00000000 <res 00000340>          backuparea resb 832
    43                                  
    44                                  segment .text
    45                                  fill_random_array:
    46                                  
    47                                  ; ============= Backup GPRs ============================
    48 00000000 55                      push rbp
    49 00000001 4889E5                  mov rbp, rsp
    50 00000004 53                      push rbx
    51 00000005 51                      push rcx
    52 00000006 52                      push rdx
    53 00000007 56                      push rsi
    54 00000008 57                      push rdi
    55 00000009 4150                    push r8
    56 0000000B 4151                    push r9
    57 0000000D 4152                    push r10
    58 0000000F 4153                    push r11
    59 00000011 4154                    push r12
    60 00000013 4155                    push r13
    61 00000015 4156                    push r14
    62 00000017 4157                    push r15
    63 00000019 9C                      pushf
    64                                  
    65                                  ; =============== xsave =================================
    66 0000001A B807000000              mov rax, 7
    67 0000001F BA00000000              mov rdx, 0
    68 00000024 0FAE2425[00000000]      xsave [backuparea]
    69                                  
    70                                  ; =============== fill_random_array =====================
    71 0000002C 4D31ED                  xor r13, r13            ; index starting at 0
    72 0000002F 4989FE                  mov r14, rdi            ; num_array
    73 00000032 4989F7                  mov r15, rsi            ; size of num_array
    74                                  
    75                                  beginloop:
    76 00000035 4D39FD                    cmp r13, r15          ; if index reaches size of num_array
    77 00000038 7D0D                      jge endloop           ; jump to end of loop
    78                                  
    79 0000003A 490FC7F4                  rdrand r12            ; generate random number and copy to r15
    80 0000003E 4F8924EE                  mov [r14+8*r13], r12
    81 00000042 49FFC5                    inc r13               ; i++
    82 00000045 EBEE                      jmp beginloop         ; jump to beginloop until index = size
    83                                  
    84                                  endloop:
    85                                  
    86                                  ; =============== xrstor ================================
    87 00000047 B807000000              mov rax, 7
    88 0000004C BA00000000              mov rdx, 0
    89 00000051 0FAE2C25[00000000]      xrstor [backuparea]
    90                                  
    91                                  ; =============== Restore GPRs ==========================
    92 00000059 9D                      popf
    93 0000005A 415F                    pop r15
    94 0000005C 415E                    pop r14
    95 0000005E 415D                    pop r13
    96 00000060 415C                    pop r12
    97 00000062 415B                    pop r11
    98 00000064 415A                    pop r10
    99 00000066 4159                    pop r9
   100 00000068 4158                    pop r8
